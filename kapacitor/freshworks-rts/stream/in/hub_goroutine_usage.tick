stream
  |from()
      .database('freshworks-rts')
      .measurement('hub_goRoutineCountHub')
  |where(lambda: "region" == 'ap-south-1')
  |groupBy('host')
  |window()
    .period(1m)
    .every(1m)
  |max('value')
    .as('value')
  |alert()
    .message('[ {{ .Time }} ] [ {{ index .Tags "region" }} ] [ {{ index .Tags "host" }} ] [ goroutine_usage_in ] - {{ index .Fields "value" }} {{ .Level }}')
    .id('{{ .Name }}:{{ .Group }}/goroutine_usage_in')
    .warn(lambda: "value" > 33000)
    .warnReset(lambda: "value" <= 32000)
    .crit(lambda: "value" > 35000)
    .critReset(lambda: "value" <= 34000)
    .stateChangesOnly()
    .topic('freshworks-rts')