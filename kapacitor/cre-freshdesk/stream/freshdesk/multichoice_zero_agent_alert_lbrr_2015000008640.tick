stream
  |from()
    .database('cre-freshdesk')
    .measurement('cre_cf_sherlock_ticket_agent_update_total')
  |where(lambda: "is_automation" == 'true' AND "group_id" == '2015000008640' AND "account_id" == '986130')
  |derivative('counter')
    .unit(10m)
    .nonNegative()
    .as('updates')
  |window()
    .period(5m)
    .every(5m)
  |mean('updates')
    .as('updates')
  |stateDuration(lambda: "updates" == 0)
    .unit(1m)
  |alert()
    .message('[ {{ .Time }} ] [multichoice_zero_agent_alert_lbrr_2015000008640] [ Zero Agent assignments for the last 10 minutes for group with id = 2015000008640 ] {{ .Level }}')
    .warn(lambda: "state_duration" >= 10 AND ((hour("time") > 4) AND ((hour("time") < 22))))
    .warnReset(lambda: "state_duration" < 10)
    .stateChangesOnly()
    .topic('cre-freshdesk')