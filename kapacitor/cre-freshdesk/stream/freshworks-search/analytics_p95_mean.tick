stream
  |from()
    .database('cre-freshdesk')
    .measurement('cre_freshdesk_freshworks-search_stats')
    .where (lambda: "c" == 'analytics')
    .groupBy('a_id','region')
  |window()
    .period(5m)
    .every(5m)
  |mean('p95_duration')
    .as('mean_p95')
  |alert()
    .message('[ {{ .Time }} ] [ {{ index .Tags "region" }} ] [ {{ index .Tags "c" }} ] [ {{ index .Tags "a_id" }} ] [ analytics_p95_mean ] - {{ index .Fields "mean_p95" }} {{ .Level }}')
    .crit(lambda: "mean_p95" >= 1000)
    .critReset(lambda: "mean_p95" < 1000)
    .stateChangesOnly()
    .topic('cre-freshdesk')