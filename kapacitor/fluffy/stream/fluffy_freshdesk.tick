stream
  |from()
    .database('fluffy')
    .measurement('cre_fluffy_FRESHDESK')
  |groupBy('account','path','region')
  |window()
    .period(30s)
    .every(30s)
  |mean('gauge')
    .as('gauge')
  |alert()
        .message( '[{{ index .Tags "account" }}] [Ratelimiting_Breached_80]' )
        .crit(lambda: "gauge" > 80)
        .stateChangesOnly()
        .topic('fluffy')
